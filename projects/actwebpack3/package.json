{
  "name": "wapqq",
  "version": "1.0.7",
  "description": "wapqq web frontend",
  "scripts": {
    "start": "npm run clean && webpack -w --progress --config ./config/webpack.dev.js",
    "build": "npm run clean && npm run buildapp",
    "build:test": "npm run clean && npm run buildapp:test",
    "buildapp": "webpack --progress --bail --env.buildType=prod --config ./config/webpack.prod.js",
    "buildapp:test": "webpack --progress --bail --env.buildType=prod --env.isTest=true --config ./config/webpack.prod.js",
    "clean": "del-cli ./build/* && del-cli ./src/sprite/spritedest/*",
    "addall": "git add -A",
    "commit": "git-cz"
  },
